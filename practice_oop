class Human{
    constructor(name,age,sex){
        this.name=name;
        this.age=age;
        this.sex=sex;
    }
    speak(){
        console.log(`bạn này tên là:${this.name}, ${this.age} tuổi, giới tính:${this.sex}`);
    }

}
const Hung= new Human('Hùng',18, 'nam');
const Quan= new Human('Quân',22, 'nam');
const Anh= new Human('Anh',4, 'nữ');
Hung.speak();
Quan.speak();
Anh.speak();