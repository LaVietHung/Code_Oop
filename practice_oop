class Calculator{
    constructor(){
        this.value=0;
    }
    plus(number){
        this.value+=number;
        return this;

    }
    subtract(number){
        this.value-=number;
        return this;
    }
    multiply(number){
        this.value*=number;
        return this;
    }
    getResult(){
        return this.value;
    }
}
const result= new Calculator().plus(5).subtract(2).multiply(3).getResult()
console.log(result);